{
  "name": "prismock",
  "version": "1.0.0",
  "description": "A mock for PrismaClient, dedicated to unit testing.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/morintd/prismock",
  "author": "Teddy Morin <morin.td@gmail.com> (http://morintd.medium.com/)",
  "license": "MIT",
  "keywords": [
    "test",
    "prisma"
  ],
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "yarn build:main && yarn build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "lint": "eslint src --ext .ts",
    "test": "jest src/ --maxWorkers=1",
    "db:reset": "prisma migrate reset -f",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@prisma/client": "5.1.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.5",
    "@semantic-release/release-notes-generator": "11.0.7",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cspell": "7.3.2",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.3.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-testing-library": "6.0.1",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "14.0.1",
    "prettier": "2.8.8",
    "prisma": "5.1.1",
    "semantic-release": "21.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "typescript": "5.2.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/generator-helper": "5.1.1",
    "@prisma/internals": "5.1.1",
    "bson": "6.0.0"
  },
  "peerDependencies": {
    "@prisma/client": "*"
  }
}
